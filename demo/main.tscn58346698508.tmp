code = "


shader_type canvas_item;

uniform float time_offset : hint_range(0, 100.0);

void fragment() {
    vec2 uv_offset = vec2(
        sin(UV.y * 10.0 + TIME * 5.0 + time_offset) * 0.0,
        cos(UV.x * 10.0 + TIME * 3.0 + time_offset) * 0.0
    );

    COLOR = texture(TEXTURE, UV + uv_offset);
}"

